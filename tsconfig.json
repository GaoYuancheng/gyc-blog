{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "outDir": "dist",
    "jsx": "react",
    "baseUrl": "./",
    "emitDeclarationOnly": true,
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "gyc-components": ["src"],
      "gyc-components/*": ["src/*", "*"]
    }
  },
  "include": ["src/**/*", ".dumirc.ts"],
  "exclude": ["src/**/**/demo/*"]
}
